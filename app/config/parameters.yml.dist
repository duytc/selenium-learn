# This file is a "template" of what your parameters.yml file should look like
# Set parameters here that may be different on each deployment target of the app, e.g. development, staging, production.
# http://symfony.com/doc/current/best_practices/configuration.html#infrastructure-related-configuration
parameters:
    tagcade.default_data_path:      ./data/fetcher

    # A secret key that's used to generate certain security-related tokens
    secret:            ThisTokenIsNotSoSecretChangeIt
    selenium_server_url: 'http://localhost:4444/wd/hub'

    tagcade_user_name: admin
    tagcade_user_password: '123456'
    tagcade_download_time_out: 30
    tagcade_get_token_url: 'http://api.tagcade.dev/app_dev.php/api/v1/getToken'
    tagcade_get_list_publisher_url: 'http://api.tagcade.dev/app_dev.php/api/v1/adnetworks/{cname}/publishers'
    ur_get_list_integrations_to_be_executed_url: 'http://api.unified-reports.dev/app_dev.php/api/v1/datasourceintegrations/byschedule'
    ur_update_last_execution_time_for_integration_url: 'http://api.unified-reports.dev/app_dev.php/api/v1/datasourceintegrations/updatelastexecutetime'
    ur_post_datasource_import_json_data: 'http://api.unified-reports.dev/app_dev.php/api/v1/datasources/{id}/importJson'

    fetcher_worker_tube: 'fetcher-worker'
    pheanstalk_ttr: 60
    pheanstalk_primary_host: 127.0.0.1
